<Window x:Class="MRTest.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MRTest"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    <Grid>
        <Slider Width="150" Margin="0 10 0 0" Maximum="180" Minimum="0" ValueChanged="Slider_ValueChanged"></Slider>
        <Slider Width="150" Margin="0 100 0 0" Maximum="180" Minimum="0" ValueChanged="Slider_ValueChanged_1"></Slider>
        
    </Grid>
</Window>
